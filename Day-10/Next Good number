#include <iostream>
#include <vector>
#include<climits>
using namespace std;
bool check_all_digits_odd(int num){
  while(num > 0){
    if(((num % 10))% 2 == 0) return false;
    num = num / 10;
  }
  return true;
}
int if_digit_nine(){
  return 1;
}
int next_good_number(int x) {
    // Write your logic here.
    int num = 0;
    while(x < 0){
      int digit = x % 10;
      x = x/10;
      if(digit % 2 ==0) digit++;
      else if(digit == 9){ digit = 1;
      x+=1;
      }
      else digit += 2;
      num = num * 10 + digit;
    }
    return num;
}

int main() {
    int T;
    std::cin >> T;
    std::vector<int> results(T);

    for (int i = 0; i < T; ++i) {
        int x;
        std::cin >> x;
        results[i] = next_good_number(x);
    }

    for (const int &res : results) {
        std::cout << res << std::endl;
    }

    return 0;
}
