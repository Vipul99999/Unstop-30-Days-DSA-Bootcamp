#include <iostream>
#include <vector>
#include <climits>
#include<algorithm>
using namespace std;

// Placeholder function for user logic
int findLargestNumber(vector<int>& arr) {
    int min1 = 10, min2 = 10;
    for (int num : arr) {
        if (num < min1) {
            min2 = min1;
            min1 = num;
        } else if (num < min2) {
            min2 = num;
        }
    }
    return max(min1 * 10 + min2, min2 * 10 + min1);
}

int main() {
    int n;
    cin >> n;
    vector<int> arr(n);
    for (int i = 0; i < n; ++i) {
        cin >> arr[i];
    }
    // Call user logic function and print the output
    int result = findLargestNumber(arr);
    cout << result << endl;
    return 0;
}
