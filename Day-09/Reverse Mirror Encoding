#include <iostream>
#include <string>

char decode_char(char ch) {
    // For 'a' to 'n'
    if (ch >= 'a' && ch <= 'n') {
        return 'n' - (ch - 'a');
    }
    // Should not reach here for '#' encoded characters
    return ch;
}

char decode_hash_char(char ch) {
    // For 'o#' to 'z#'
    if (ch >= 'o' && ch <= 'z') {
        return 'z' - (ch - 'o');
    }
    return ch;
}

std::string decode_message(const std::string& S) {
    std::string result;
    for (size_t i = 0; i < S.length(); ++i) {
        if (i + 1 < S.length() && S[i + 1] == '#') {
            result.push_back(decode_hash_char(S[i]));
            i++; // Skip the '#' character
        } else {
            result.push_back(decode_char(S[i]));
        }
    }
    return result;
}

int main() {
    std::string S;
    std::cin >> S;
    std::string decoded = decode_message(S);
    std::cout << decoded << std::endl;
    return 0;
}
