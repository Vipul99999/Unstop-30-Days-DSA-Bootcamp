//CPP
#include <iostream>
#include <vector>

int calculate_max_moons(int n, int m, const std::vector<std::vector<int>>& moons) {
    int max_moons = 0;

    for (int i = 0; i < n; ++i) {
        int total_moons = 0;
        for (int j = 0; j < m; ++j) {
            total_moons += moons[i][j];
        }
        if (total_moons > max_moons) {
            max_moons = total_moons;
        }
    }

    return max_moons;
}

int main() {
    int n, m;
    std::cin >> n >> m;
    
    std::vector<std::vector<int>> moons(n, std::vector<int>(m));
    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < m; ++j) {
            std::cin >> moons[i][j];
        }
    }
    
    int result = calculate_max_moons(n, m, moons);
    std::cout << result << std::endl;
    return 0;
}
