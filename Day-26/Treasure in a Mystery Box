#include <iostream>
#include <vector>

int get_treasure(int n) {
    if (n == 1) return 1;
    if (n == 2) return 2;

    int a = 1; // ways to reach compartment 1
    int b = 2; // ways to reach compartment 2
    int ways = 0;

    for (int i = 3; i <= n; ++i) {
        ways = a + b;
        a = b;
        b = ways;
    }
    return ways;
}

int main() {
    int n;
    std::cin >> n;
    
    int result = get_treasure(n);
    std::cout << result << std::endl;
    
    return 0;
}
